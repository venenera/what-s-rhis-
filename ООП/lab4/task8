#include <iostream>
using namespace std;
void swap(int &num1, int &num2)
{
	int t = num1;
	num1 = num2;
	num2 = t;
}
int main()
{
	setlocale(LC_ALL, "rus");
	int number1, number2;
	cout << "Введите два числа: ";
	cout << "1: ";
	cin >> number1;
	cout << "2: ";
	cin >> number2;
	swap(number1, number2);
	cout << "Результат обмена: " << number1 << ", " << number2;
	return 0;
}
